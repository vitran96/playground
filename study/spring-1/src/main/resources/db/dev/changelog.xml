<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet author="vitran96" id="init" context="spring-1" labels="dev">
        <sqlFile path="init.up.sql"
                 relativeToChangelogFile="true" stripComments="true" splitStatements="false"/>
        <rollback>
            <sqlFile path="init.down.sql"
                     relativeToChangelogFile="true" stripComments="true" splitStatements="false"/>
        </rollback>
    </changeSet>
    <changeSet author="vitran96" id="create-user" context="spring-1" labels="dev">
        <sqlFile path="create-user.up.sql"
                 relativeToChangelogFile="true" stripComments="true" splitStatements="false"/>
        <rollback>
            <sqlFile path="create-user.down.sql"
                     relativeToChangelogFile="true" stripComments="true" splitStatements="false"/>
        </rollback>
    </changeSet>
</databaseChangeLog>
